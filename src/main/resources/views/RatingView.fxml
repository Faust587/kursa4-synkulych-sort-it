<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<VBox styleClass="RatingView"
      fx:controller="ua.synkulych.sort_it.controllers.RatingController"
      xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1">
    <Label styleClass="BigLabel"
           text="%rating_label"/>
    <ScrollPane fx:id="RatingPane">
        <GridPane fx:id="RatingList" gridLinesVisible="true">
            <columnConstraints>

            </columnConstraints>
            <rowConstraints>

            </rowConstraints>
        </GridPane>
    </ScrollPane>
    <Button onMouseClicked="#OpenMenu"
            styleClass="ExitButton"
            text="%back_button"/>
</VBox>
